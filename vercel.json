{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "/src/public/$1"
    },
    {
      "src": "/views/(.*)",
      "dest": "/src/views/$1"
    },
    {
      "src": "/routes/(.*)",
      "dest": "/src/routes/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
